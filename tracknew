prg7
mkdir rmr
cd rmr
touch m1.txt
git add m1.txt
git commit -m "tagging"
git tag v1.0
git log --oneline


prg8
git branch
git checkout -b "source-branch"
vi file.txt
git add file.txt
git commit -m "picking " x2
git log --oneline
git checkout "master"
git cherry-pick commitID^..COMMITID
git log --oneline to check

Prg9
git log --oneline
git show COMMITID - method one
git log -n 1 COMMITID - method 2


Prg10
git log --author="Rakshith" --since="2024-01-01" --until="2024-12-31"

Prg11
git log -n 5 or 3(any number)

Prg12
git revert COMMITID
exit out of it
